{
  "name": "eonx-com/sdkblueprint",
  "description": "A library for quickly building an SDK for a RESTful API",
  "keywords": [
    "loyaltycorp",
    "api",
    "sdk"
  ],
  "license": "BSD-3-Clause",
  "type": "package",
  "require": {
    "php": ">=7.1",
    "eonx-com/utils": "^1.0",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "guzzlehttp/guzzle": "^6.0",
    "symfony/serializer": "^4.1",
    "symfony/property-access": "^4.1",
    "symfony/property-info": "^4.1",
    "doctrine/annotations": "^1.6",
    "doctrine/cache": "^1.7",
    "symfony/intl": "^4.1",
    "symfony/expression-language": "^4.1"
  },
  "require-dev": {
    "indigophp/doctrine-annotation-autoload": "^0.1.0",
    "eonx-com/standards": "^0.2",
    "friendsofphp/php-cs-fixer": "^2.9",
    "php-http/guzzle6-adapter": "^1.1",
    "phpmd/phpmd": "^2.6",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^7.0",
    "roave/security-advisories": "dev-master",
    "sebastian/phpcpd": "^4.0",
    "squizlabs/php_codesniffer": "3.*",
    "woohoolabs/yang": "^1.2"
  },
  "replace": {
    "loyaltycorp/sdkblueprint": "*"
  },
  "suggest": {
    "sensiolabs/security-checker": "Check project's dependencies for known vulnerabilities"
  },
  "autoload": {
    "psr-4": {
      "LoyaltyCorp\\SdkBlueprint\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\LoyaltyCorp\\SdkBlueprint\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
